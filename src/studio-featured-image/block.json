{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ls/studio-featured-image",
	"version": "0.1.0",
	"title": "LS Studio Featured Image",
	"category": "la-saphire",
	"description": "LS Studio Featured Image Display.",
	"parent": ["ls/studio-list"],
	"attributes": {
		"isLink": {
			"type": "boolean",
			"default": false
		},
		"width": {
			"type": "string"
		},
		"height": {
			"type": "string"
		},
		"scale": {
			"type": "string",
			"defaultt": "cover"
		},
		"sizeSlug": {
			"type": "string"
		}
	},
	"usesContext": ["postId", "postType"],
	"supports": {
			"html": false
	},
	"textdomain": "ls-studio-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
